{
    "manifest_version": 2,
    "name": "GCM Test",
    "description": "It's test extension",
    "version": "1.0.12",

    "permissions": [
        "pushMessaging",
        "notifications",
        "tabs"
    ],
    "background": {
        "scripts": ["test.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*"
            ],
            "js": ["content_script.js", "jquery.min.js"],
            "run_at": "document_end"
        }
    ]
}