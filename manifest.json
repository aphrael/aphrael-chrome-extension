{
    "manifest_version": 2,
    "name": "Aphrael",
    "description": "Aphrael for chrome",
    "version": "0.0.13",
    "permissions": [
        "pushMessaging",
        "tabs",
        "http://*/"
    ],
    "background": {
        "scripts": ["notify.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*"
            ],
            "js": ["content_script.js", "jquery.min.js"],
            "run_at": "document_end"
        }
    ]
}